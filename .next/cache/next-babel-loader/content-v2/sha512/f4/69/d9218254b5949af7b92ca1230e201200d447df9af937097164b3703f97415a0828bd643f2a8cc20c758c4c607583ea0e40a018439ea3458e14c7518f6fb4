key={index}>\n          <a className={styles.menu_container}>\n            <img className={styles.menu_img} src={val.iconClass} />\n            <div className={styles.menu_text}>{val.moduleName}</div>\n          </a>\n        </Link>\n      );\n    });\n    return render;\n  };\n\n  const renderPlan = () => {\n    var now = new Date();\n    var date = now.getDate();\n    var month = now.getMonth() + 1;\n    var year = now.getFullYear();\n    if (loading) {\n      return <Spinner />;\n    } else {\n      return (\n        <>\n          <Card radius={5}>\n            <div className={styles.overview}>\n              <div>\n                <span\n                  className={styles.date}\n                >{`${date} / ${month} / ${year}`}</span>\n                <div style={{ color: \"#5E5873\", marginTop: \"7px\" }}>\n                  <span style={{ fontSize: \"36px\", fontWeight: \"600\" }}>3</span>\n                  <span style={{ fontSize: \"18px\" }}>/13</span>\n                </div>\n                <button\n                  style={{\n                    marginTop: \"7px\",\n                    backgroundColor: \"#FEB800\",\n                    padding: \"10px 22px\",\n                    fontSize: \"14px\",\n                    fontWeight: \"400\",\n                    color: \"#fff\",\n                    border: \"none\",\n                    borderRadius: \"5px\",\n                  }}\n                >\n                  Add New Plan +\n                </button>\n              </div>\n              <div></div>\n            </div>\n          </Card>\n          <Card radius={30}>\n            <div className={styles.plan_container}>\n              <div\n                style={{\n                  fontSize: \"18px\",\n                  fontWeight: \"600\",\n                  color: \"#5E5873\",\n                  textAlign: \"left\",\n                }}\n              >\n                Your Plan Today\n              </div>\n              <Button text={\"See Details\"} />\n            </div>\n          </Card>\n        </>\n      );\n    }\n  };\n  const renderUnplan = () => {};\n  const renderSpreading = () => {};\n\n  const renderPage = () => {\n    if (state.menuReducer.menu == null) {\n      return <Spinner />;\n    } else\n      return (\n        <div>\n          <Head>\n            <title>Home</title>\n            <link rel=\"icon\" href=\"/favicon.ico\" />\n          </Head>\n          <div className={styles.container}>\n            <div className={styles.wrapper}>\n              <div className={styles.bg_top} />\n              <div className={styles.user_info_grid}>\n                <img className={styles.pp} src=\"/pp.png\" />\n                <div style={{ marginLeft: \"15px\" }}>\n                  <div style={{ fontSize: \"14px\" }}>\n                    {state.userReducer.user.name}\n                  </div>\n                  <div style={{ fontSize: \"12px\" }}>{\"user.role**\"}</div>\n                </div>\n                <img className={styles.notif} src=\"/notif.svg\" />\n                <div>\n                  <div className={styles.menu_grid}>\n                    <div\n                      className={\n                        focus === \"plan\" ? styles.focus_menu : styles.menu\n                      }\n                      onClick={() => {\n                        setFocus(\"plan\");\n                      }}\n                    >\n                      Plan\n                    </div>\n                    <div\n                      className={\n                        focus === \"unplan\" ? styles.focus_menu : styles.menu\n                      }\n                      onClick={() => {\n                        setFocus(\"unplan\");\n                      }}\n                    >\n                      Unplan\n                    </div>\n                    <div\n                      className={\n                        focus === \"spreading\" ? styles.focus_menu : styles.menu\n            