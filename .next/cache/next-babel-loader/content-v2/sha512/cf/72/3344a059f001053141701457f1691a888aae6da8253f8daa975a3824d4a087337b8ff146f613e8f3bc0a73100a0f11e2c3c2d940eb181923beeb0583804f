${router.query.id}`);\r\n    } else if (type === \"UNPLAN\") {\r\n      Router.push(`/visit/unplan/submit`);\r\n    } else if (type === \"SPREADING\") {\r\n      Router.push(`/visit/spreading/submit`);\r\n    }\r\n  };\r\n  const render = () => {\r\n    return (\r\n      <>\r\n        {renderModalAdd()}\r\n        <div className={styles.container}>\r\n          <div className={styles.wrapper}>\r\n            <Nav\r\n              title={\"Avability\"}\r\n              color={\"white\"}\r\n              action={\"Save\"}\r\n              onClick={() => {\r\n                onSave();\r\n              }}\r\n              backAction={() => {\r\n                Router.back();\r\n              }}\r\n              disable={false}\r\n            />\r\n            <div className={styles.main}>\r\n              <div className={styles.search_fixed}>\r\n                <div style={{ margin: \"10px 0 0 0\" }}>\r\n                  <div className={styles.progress_bar}></div>\r\n                  <div\r\n                    className={styles.progress_bar_now}\r\n                    style={{\r\n                      width: `${(avabilityList.length / 25) * 100}%`,\r\n                    }}\r\n                  ></div>\r\n                </div>\r\n                <input\r\n                  onChange={(e) => {\r\n                    setSearch(e.target.value);\r\n                  }}\r\n                  placeholder=\"Search\"\r\n                  className={styles.input}\r\n                  style={{ marginTop: \"0\" }}\r\n                />\r\n              </div>\r\n              <div style={{ paddingTop: \"60px\" }}>\r\n                {renderAvabilityList()}\r\n                <div style={{ marginBottom: \"100px\" }} />\r\n              </div>\r\n            </div>\r\n            <div className={styles.avability_bot_container}>\r\n              <Button\r\n                text={\"Add\"}\r\n                onClick={() => {\r\n                  setModal(true);\r\n                  setProduct([]);\r\n                  setProductFocus(\"\");\r\n                }}\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </>\r\n    );\r\n  };\r\n  return render();\r\n}\r\n"]},"metadata":{},"sourceType":"module"}