n                if (i === 7) {\r\n                  setLoadingSubmit(false);\r\n                  Router.push(\"/\");\r\n                  actions.setDefaultVisitPlan();\r\n                }\r\n              })\r\n              .catch((err) => {\r\n                console.log(err);\r\n              });\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    } else {\r\n      setVisNotDone(true);\r\n    }\r\n  };\r\n\r\n  const render = () => {\r\n    const visDone = state.visitPlanReducer.visibility.filter((val) => {\r\n      return val.file !== null && val.type !== null && val.brand !== null;\r\n    });\r\n\r\n    if (loading) {\r\n      return <Spinner />;\r\n    } else {\r\n      return (\r\n        <>\r\n          <div className={styles.container}>\r\n            {loadingSubmit ? (\r\n              <Modal>\r\n                <Spinner />\r\n              </Modal>\r\n            ) : null}\r\n            <div className={styles.wrapper}>\r\n              <Nav\r\n                title={\"Plan\"}\r\n                color={\"white\"}\r\n                action={\"Submit\"}\r\n                onClick={() => {\r\n                  onSubmit();\r\n                }}\r\n                disable={visDone.length === 8 ? false : true}\r\n                backAction={() => {\r\n                  if (\r\n                    confirm(\r\n                      \"Data will be lost if you leave the page, are you sure?\"\r\n                    )\r\n                  ) {\r\n                    actions.setDefaultVisitPlan();\r\n                    Router.push(\"/visit/plan\");\r\n                  }\r\n                }}\r\n              />\r\n              <div className={styles.main}>\r\n                {visNotDone ? (\r\n                  <div\r\n                    style={{\r\n                      color: \"red\",\r\n                      fontSize: \"13px\",\r\n                      textAlign: \"center\",\r\n                    }}\r\n                  >\r\n                    Please complete visibility data\r\n                  </div>\r\n                ) : (\r\n                  \"\"\r\n                )}\r\n                {renderDetail()}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </>\r\n      );\r\n    }\r\n  };\r\n\r\n  return render();\r\n}\r\n"]},"metadata":{},"sourceType":"module"}