            setDummy(dummy + 1);\r\n              }}\r\n              value={val.type != null ? val.type.namaFile : \"\"}\r\n            />\r\n            <span\r\n              style={{\r\n                fontSize: \"10px\",\r\n                color: \"#41867A\",\r\n                fontWeight: \"500\",\r\n              }}\r\n            >\r\n              {val.file != null ? val.file.name : null}\r\n            </span>\r\n          </div>\r\n          <div>\r\n            <label className={styles.new_button} htmlFor={`upload${index}`}>\r\n              <img\r\n                src={\"/camera.svg\"}\r\n                style={{ width: \"18px\", verticalAlign: \"baseline\" }}\r\n              />\r\n              <input\r\n                className={styles.input_file}\r\n                onChange={(e) => {\r\n                  vis.splice(index, 1, {\r\n                    ...vis[index],\r\n                    file: e.target.files[0],\r\n                  });\r\n                  setDummy(dummy + 1);\r\n                }}\r\n                id={`upload${index}`}\r\n                type=\"file\"\r\n                accept=\"image/*\"\r\n                capture=\"environment\"\r\n              />\r\n            </label>\r\n          </div>\r\n        </div>\r\n      );\r\n    });\r\n    return render;\r\n  };\r\n  const renderHistoryPosm = () => {\r\n    var render = posmList.map((val, index) => {\r\n      console.log(val);\r\n      return (\r\n        <div key={index} className={styles.visibility_grid}>\r\n          <div>POSM {index + 1}</div>\r\n          <div className={styles.visibility_dropdown}>\r\n            <Card\r\n              style={{\r\n                height: \"32px\",\r\n                border: \"1px solid #e9ecf2\",\r\n                padding: \"6px 10px\",\r\n                borderRadius: \"5px\",\r\n              }}\r\n            >\r\n              {val.tipe}\r\n            </Card>\r\n            <span\r\n              style={{\r\n                fontSize: \"10px\",\r\n                color: \"#41867A\",\r\n                fontWeight: \"500\",\r\n              }}\r\n            >\r\n              {val.namaFile}\r\n            </span>\r\n          </div>\r\n        </div>\r\n      );\r\n    });\r\n    return render;\r\n  };\r\n  const onSave = () => {\r\n    if (type === \"PLAN\") {\r\n      actions.setPlanVisibility(vis);\r\n      // console.log(vis);\r\n      Router.push(`/visit/plan/${router.query.id}`);\r\n    } else if (type === \"UNPLAN\") {\r\n      Router.push(`/visit/unplan/submit`);\r\n    } else if (type === \"SPREADING\") {\r\n      Router.push(`/visit/spreading/submit`);\r\n    }\r\n  };\r\n  const render = () => {\r\n    return (\r\n      <div className={styles.container}>\r\n        <div className={styles.wrapper}>\r\n          {type === \"HISTORY\" ? (\r\n            <Nav\r\n              title={\"Visibility\"}\r\n              color={\"white\"}\r\n              backAction={() => {\r\n                Router.back();\r\n              }}\r\n            />\r\n          ) : (\r\n            <Nav\r\n              title={\"Visibility\"}\r\n              color={\"white\"}\r\n              action={\"Save\"}\r\n              onClick={() => {\r\n                onSave();\r\n              }}\r\n              backAction={() => {\r\n                if (state.visitPlanReducer.visibility.length > 0) {\r\n                  setVis([...state.visitPlanReducer.visibility]);\r\n                } else {\r\n                  setVis([...initialVis]);\r\n                }\r\n                Router.back();\r\n              }}\r\n              disable={false}\r\n            />\r\n          )}\r\n          <div className={styles.main}>\r\n            {type === \"HISTORY\" ? renderHistoryPosm() : renderInputUpload()}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    );\r\n  };\r\n  return render();\r\n}\r\n"]},"metadata":{},"sourceType":"module"}