    <div style={{ color: \"#5E5873\", marginTop: \"7px\" }}>\r\n                  <span style={{ fontSize: \"36px\", fontWeight: \"600\" }}>\r\n                    {workVisitHistory.length}\r\n                  </span>\r\n                  <span style={{ fontSize: \"18px\" }}>\r\n                    /{workVisit.length + workVisitHistory.length}\r\n                  </span>\r\n                </div>\r\n                <button\r\n                  style={{\r\n                    marginTop: \"7px\",\r\n                    backgroundColor: \"#FEB800\",\r\n                    padding: \"10px 22px\",\r\n                    fontSize: \"14px\",\r\n                    fontWeight: \"400\",\r\n                    color: \"#fff\",\r\n                    border: \"none\",\r\n                    borderRadius: \"5px\",\r\n                  }}\r\n                >\r\n                  Add New +\r\n                </button>\r\n              </div>\r\n              <div>\r\n                <div className={styles.pie_percentage}>\r\n                  {workVisitHistory.length === 0\r\n                    ? \"0%\"\r\n                    : `${Math.round(\r\n                        (workVisitHistory.length /\r\n                          (workVisit.length + workVisitHistory.length)) *\r\n                          100\r\n                      )}%`}\r\n                </div>\r\n                <Doughnut\r\n                  data={dataWorkVisit}\r\n                  options={donutOptions}\r\n                  width={100}\r\n                  height={100}\r\n                />\r\n              </div>\r\n            </div>\r\n          </Card>\r\n          <Card style={{ marginTop: \"22px\", borderRadius: \"30px\" }} shadow>\r\n            <div className={styles.plan_container}>\r\n              <div\r\n                style={{\r\n                  fontSize: \"18px\",\r\n                  fontWeight: \"500\",\r\n                  color: \"#5E5873\",\r\n                  textAlign: \"left\",\r\n                }}\r\n              >\r\n                Your Work Visit Today\r\n              </div>\r\n              <div style={{ margin: \"22px 0\" }}>\r\n                {renderListWorkVisit(workVisit)}\r\n              </div>\r\n              <Link href=\"/work-visit\">\r\n                <a>\r\n                  <Button text={\"See Details\"} />\r\n                </a>\r\n              </Link>\r\n            </div>\r\n          </Card>\r\n        </>\r\n      );\r\n    }\r\n  };\r\n\r\n  const renderPage = () => {\r\n    if (loading && loadingMenu) {\r\n      return <Spinner />;\r\n    } else {\r\n      return (\r\n        <div>\r\n          <Head>\r\n            <title>Home</title>\r\n            <link rel=\"icon\" href=\"/favicon.ico\" />\r\n          </Head>\r\n          <div className={styles.container}>\r\n            <div className={styles.wrapper}>\r\n              <div className={styles.bg_top} />\r\n              <div className={styles.user_info_grid}>\r\n                <img className={styles.pp} src=\"/pp.png\" />\r\n                <div style={{ marginLeft: \"15px\" }}>\r\n                  <div style={{ fontSize: \"14px\" }}>\r\n                    {state.userReducer.user.name}\r\n                  </div>\r\n                  <div style={{ fontSize: \"12px\" }}>{role}</div>\r\n                </div>\r\n                <Link href={\"/announcement\"}>\r\n                  <a>\r\n                    <img className={styles.notif} src=\"/notif.svg\" />\r\n                  </a>\r\n                </Link>\r\n                <div className={styles.main}>\r\n                  {renderTopMenu()}\r\n                  {focus === \"PLAN\"\r\n                    ? renderPlan()\r\n                    : focus === \"UNPLAN\"\r\n                    ? renderUnplan()\r\n                    : focus === \"SPREADING\"\r\n                    ? renderSpreading()\r\n                    : focus === \"WORK-VISIT\"\r\n                    ? renderWorkVisit()\r\n                    : \"\"}\r\n                  <div style={{ marginBottom: \"120px\" }} />\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <BotNav />\r\n        </div>\r\n      );\r\n    }\r\n  };\r\n  return renderPage();\r\n}\r\n"]},"metadata":{},"sourceType":"module"}