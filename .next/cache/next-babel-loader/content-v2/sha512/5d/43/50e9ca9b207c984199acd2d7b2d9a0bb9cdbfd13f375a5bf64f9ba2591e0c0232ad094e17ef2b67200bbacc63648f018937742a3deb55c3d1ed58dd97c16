State([]);\n  const [loading, setLoading] = useState(true);\n  useEffect(() => {\n    const userData = JSON.parse(localStorage.getItem(\"user\"));\n    const userMenu = JSON.parse(localStorage.getItem(\"menu\"));\n    if (userData) {\n      actions.userLogin(userData);\n      actions.setMenu(userMenu);\n      fetch(\n        `http://10.100.4.116:8229/api/user/getmenu?username=${userData.email}`\n      )\n        .then((response) => {\n          return response.json();\n        })\n        .then((data) => {\n          actions.setMenu(data);\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    } else {\n      Router.push(\"/\");\n    }\n  }, [focus]);\n\n  useEffect(() => {\n    fetch(\n      \"http://10.100.4.116:8230/api/ActivityVisitPlan/GetAllActivityVisitPlan\"\n    )\n      .then((response) => {\n        return response.json();\n      })\n      .then((data) => {\n        setPlan(data);\n        setLoading(false);\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n  }, [focus]);\n\n  const renderMainMenu = () => {\n    var menuList =\n      state && state.menuReducer && state.menuReducer.menu\n        ? state.menuReducer.menu\n        : [];\n    const parentMenu = menuList.filter((val) => {\n      return val.isParentMenu === true;\n    });\n    const render = parentMenu.map((val, index) => {\n      return (\n        <Link href={\"/\" + val.moduleName.toLowerCase()} key={index}>\n          <a className={styles.menu_container}>\n            <img className={styles.menu_img} src={val.iconClass} />\n            <div className={styles.menu_text}>{val.moduleName}</div>\n          </a>\n        </Link>\n      );\n    });\n    return render;\n  };\n\n  const renderPlan = () => {\n    var now = new Date();\n    var date = now.getDate();\n    var month = now.getMonth() + 1;\n    var year = now.getFullYear();\n    console.log(plan);\n    const planMap = plan.map((val) => {\n      return (\n        <>\n          <div\n            style={{\n              fontSize: \"15px\",\n              fontWeight: \"700\",\n              color: \"#5E5873\",\n              textAlign: \"left\",\n              margin: \"10px 0\",\n            }}\n          >\n            <div>{val.outlet}</div>\n            <div style={{ fontSize: \"14px\", fontWeight: \"300\" }}>\n              val.address**\n            </div>\n          </div>\n        </>\n      );\n    });\n\n    if (loading) {\n      return <Spinner />;\n    } else {\n      return (\n        <>\n          <Card radius={5}>\n            <div className={styles.overview}>\n              <div>\n                <span\n                  className={styles.date}\n                >{`${date} / ${month} / ${year}`}</span>\n                <div style={{ color: \"#5E5873\", marginTop: \"7px\" }}>\n                  <span style={{ fontSize: \"36px\", fontWeight: \"600\" }}>0</span>\n                  <span style={{ fontSize: \"18px\" }}>/{plan.length}</span>\n                </div>\n                <button\n                  style={{\n                    marginTop: \"7px\",\n                    backgroundColor: \"#FEB800\",\n                    padding: \"10px 22px\",\n                    fontSize: \"14px\",\n                    fontWeight: \"400\",\n                    color: \"#fff\",\n                    border: \"none\",\n                    borderRadius: \"5px\",\n                  }}\n                >\n                  Add New Plan +\n                </button>\n              </div>\n              <div></div>\n            </div>\n          </Card>\n          <Card radius={30}>\n            <div className={styles.plan_container}>\n              <div\n                style={{\n                  fontSize: \"18px\",\n                  fontWeight: \"600\",\n                  color: \"#5E5873\",\n                  textAlign: \"left\",\n                }}\n              >\n                Your Plan Today\n              </div>\n              <div style={{ ma