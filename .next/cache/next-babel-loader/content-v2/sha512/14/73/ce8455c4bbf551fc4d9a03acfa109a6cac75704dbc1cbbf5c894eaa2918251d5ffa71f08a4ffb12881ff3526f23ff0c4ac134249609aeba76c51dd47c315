t renderList = (data) => {\r\n    return data.map((val, index) => {\r\n      return (\r\n        <div\r\n          style={{\r\n            fontSize: \"15px\",\r\n            fontWeight: \"700\",\r\n            color: \"#5E5873\",\r\n            textAlign: \"left\",\r\n            margin: \"10px 0\",\r\n          }}\r\n          key={index}\r\n        >\r\n          <div style={{ display: \"grid\", gridTemplateColumns: \"12% 88%\" }}>\r\n            <div\r\n              style={{\r\n                width: \"20px\",\r\n                height: \"20px\",\r\n                backgroundColor: \"#d1e4e1\",\r\n                borderRadius: \"20px\",\r\n                padding: \"4px\",\r\n              }}\r\n            >\r\n              <div\r\n                style={{\r\n                  width: \"12px\",\r\n                  height: \"12px\",\r\n                  backgroundColor: \"#41867a\",\r\n                  borderRadius: \"20px\",\r\n                }}\r\n              />\r\n            </div>\r\n            <div>\r\n              <div>{val.namaOutlet}</div>\r\n              <div style={{ fontSize: \"14px\", fontWeight: \"300\" }}>\r\n                {val.alamatOutlet}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      );\r\n    });\r\n  };\r\n\r\n  const renderPlan = () => {\r\n    if (loading) {\r\n      return <Spinner />;\r\n    } else {\r\n      return (\r\n        <>\r\n          <Card style={{ borderRadius: \"5px\", marginTop: \"22px\" }}>\r\n            <div className={styles.overview}>\r\n              <div>\r\n                <span className={styles.date}>{`${date} / ${\r\n                  monthName.part[month - 1]\r\n                } / ${year}`}</span>\r\n                <div style={{ color: \"#5E5873\", marginTop: \"7px\" }}>\r\n                  <span style={{ fontSize: \"36px\", fontWeight: \"600\" }}>0</span>\r\n                  <span style={{ fontSize: \"18px\" }}>/{plan.length}</span>\r\n                </div>\r\n                <button\r\n                  style={{\r\n                    marginTop: \"7px\",\r\n                    backgroundColor: \"#FEB800\",\r\n                    padding: \"10px 22px\",\r\n                    fontSize: \"14px\",\r\n                    fontWeight: \"400\",\r\n                    color: \"#fff\",\r\n                    border: \"none\",\r\n                    borderRadius: \"5px\",\r\n                  }}\r\n                >\r\n                  Add New Plan +\r\n                </button>\r\n              </div>\r\n              <div>\r\n                <div className={styles.pie_percentage}>\r\n                  {plan.length === 0 ? \"0%\" : \"0%\"}\r\n                </div>\r\n                <Doughnut\r\n                  data={dataPlan}\r\n                  options={donutOptions}\r\n                  width={100}\r\n                  height={100}\r\n                />\r\n              </div>\r\n            </div>\r\n          </Card>\r\n          <Card style={{ marginTop: \"22px\", borderRadius: \"30px\" }}>\r\n            <div className={styles.plan_container}>\r\n              <div\r\n                style={{\r\n                  fontSize: \"18px\",\r\n                  fontWeight: \"500\",\r\n                  color: \"#5E5873\",\r\n                  textAlign: \"left\",\r\n                }}\r\n              >\r\n                Your Plan Today\r\n              </div>\r\n              <div style={{ margin: \"22px 0\" }}>{renderList(plan)}</div>\r\n              <Link href=\"/visit/plan\">\r\n                <a>\r\n                  <Button text={\"See Details\"} />\r\n                </a>\r\n              </Link>\r\n            </div>\r\n          </Card>\r\n        </>\r\n      );\r\n    }\r\n  };\r\n\r\n  const renderSpreading = () => {\r\n    if (loading) {\r\n      return <Spinner />;\r\n    } else {\r\n      return (\r\n        <>\r\n          <Card style={{ borderRadius: \"5px\", marginTop: \"22px\" }}>\r\n            <div className={styles.overview}>\r\n              <div>\r\n                <span className={styles.date}>{`${date} / ${\r\n                  monthName.part[month - 1]\r\n                } / ${year}`}</span>\r\n                <div style={{ color: \"#5E5873\", marginTop: \"7px\" }}>\r\n                  <span style={{ fontSize: \"36px\", fontWeight: \"600\" }}>0</span>\r\n                  <span style={{ fontSize: \"18px\" }}>/{plan.length}</span>\r\n                </div>\r\n                <Link href={\"/visit/spreading/\"}>\r\n                  <a>\r\n                    <button\r\n                      style={{\r\n                        marginTop: \"7px\",\r\n                        backgroundColor: \"#FEB800\",\r\n                        padding: \"10px 22px\",\r\n                        fontSize: \"14px\",\r\n                        fontWeight: \"400\",\r\n                        color: \"#fff\",\r\n                        border: \"none\",\r\n                        borderRadius: \"5px\",\r\n                      }}\r\n                    >\r\n                      Add New NOO +\r\n                    </button>\r\n                  </a>\r\n                </Link>\r\n              </div>\r\n              <div>\r\n                <div className={styles.pie_percentage}>0%</div>\r\n                <Doughnut\r\n                  data={dataSpreading}\r\n                  options={donutOptions}\r\n                  width={100}\r\n                  height={100}\r\n                />\r\n              </div>\r\n            </div>\r\n          </Card>\r\n          <Card style={{ marginTop: \"22px\", borderRadius: \"30px\" }}>\r\n            <div className={styles.plan_container}>\r\n              <div\r\n                style={{\r\n                  fontSize: \"18px\",\r\n                  fontWeight: \"500\",\r\n                  color: \"#5E5873\",\r\n                  textAlign: \"left\",\r\n                }}\r\n              >\r\n                Spreading Histor