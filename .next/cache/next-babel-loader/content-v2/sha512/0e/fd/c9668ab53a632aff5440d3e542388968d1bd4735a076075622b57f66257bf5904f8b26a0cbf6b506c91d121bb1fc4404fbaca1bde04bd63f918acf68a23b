        </div>\r\n      );\r\n    } else {\r\n      return render;\r\n    }\r\n  };\r\n  const render = () => {\r\n    if (loading) {\r\n      return <Spinner />;\r\n    } else {\r\n      return (\r\n        <div className={styles.container}>\r\n          {pdfDownload ? (\r\n            <Modal\r\n              onClick={() => {\r\n                setPdfDownload(false);\r\n              }}\r\n            >\r\n              <div\r\n                style={{\r\n                  maxHeight: \"100%\",\r\n                  overflow: \"scroll\",\r\n                  overflowX: \"hidden\",\r\n                }}\r\n              >\r\n                <div\r\n                  style={{\r\n                    width: \"400px\",\r\n                    maxHeight: \"100%\",\r\n                    margin: \"100px auto\",\r\n                  }}\r\n                >\r\n                  <Invoice />\r\n                  <div style={{ padding: \"10px\", backgroundColor: \"white\" }}>\r\n                    <Button onClick={() => print()} text={\"Download PDF\"} />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </Modal>\r\n          ) : null}\r\n          <div className={styles.wrapper}>\r\n            <Nav\r\n              title={\"History\"}\r\n              backAction={() => Router.back()}\r\n              color={\"white\"}\r\n            />\r\n            <div className={styles.main}>\r\n              <div className={styles.search_fixed}>\r\n                <input\r\n                  onChange={(e) => {\r\n                    setSearch(e.target.value);\r\n                  }}\r\n                  placeholder=\"Search\"\r\n                  className={styles.input}\r\n                  style={{ marginTop: \"0\" }}\r\n                />\r\n              </div>\r\n              <div style={{ margin: \"60px 0\" }}>{renderList()}</div>\r\n              <button\r\n                onClick={() => {\r\n                  setPdfDownload(true);\r\n                }}\r\n              >\r\n                print\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      );\r\n    }\r\n  };\r\n  return render();\r\n}\r\n"]},"metadata":{},"sourceType":"module"}