ile(id) {\n  console.log(id);\n  return fetch(API_URL + API_VISIT_PLAN + \"/ActivityVisitPlanDPOSM/ViewFile/\".concat(id), {\n    headers: {\n      apiKey: TOKEN\n    }\n  }).then(function (response) {\n    return response.blob();\n  }).then(function (data) {\n    var outside = URL.createObjectURL(data);\n    return outside;\n  })[\"catch\"](function (err) {\n    console.log(err);\n  });\n};\n\nvar getPlanMonthlyHistory = function getPlanMonthlyHistory(userData) {\n  return fetch(API_URL + API_VISIT_PLAN + \"/ActivityVisitPlan/GetHistoryActivityVisitPlanBy?username=\".concat(userData.username), {\n    headers: {\n      apiKey: TOKEN\n    }\n  }).then(function (response) {\n    return response.json();\n  }).then(function (data) {\n    data.sort(function (a, b) {\n      var dateA = new Date(a.tanggal),\n          dateB = new Date(b.tanggal);\n      return dateB - dateA;\n    });\n    return data;\n  })[\"catch\"](function (err) {\n    console.log(err);\n  });\n};\n\nvar getUnplanMonthlyHistory = function getUnplanMonthlyHistory(userData) {\n  return fetch(API_URL + API_VISIT_UNPLAN + \"/SakamorActivityVisitUnPlan/GetHistoryActivityVisitUnPlanBy?username=\".concat(userData.username), {\n    headers: {\n      apiKey: TOKEN\n    }\n  }).then(function (response) {\n    return response.json();\n  }).then(function (data) {\n    console.log(\"asdf\", data);\n    data.sort(function (a, b) {\n      var dateA = new Date(a.tanggal),\n          dateB = new Date(b.tanggal);\n      return dateB - dateA;\n    });\n    return data;\n  })[\"catch\"](function (err) {\n    console.log(err);\n  });\n};\n\nexport { getMenu, getPlanList, getAuth, onLogin, getPlanId, getPosm, getProductSearch, getPlanHistory, submitVisitPlan, submitVisitPlanDposm, getProdukByJenisChannel, getInvoiceData, getInvoiceDataPosm, getPlanMonthlyHistory, getUnplanMonthlyHistory, viewFile };","map":{"version":3,"sources":["C:/Users/Rico/Desktop/Home/Kalbe app/sakamor-new/helper.js"],"names":["API_URL","API_MASTER","