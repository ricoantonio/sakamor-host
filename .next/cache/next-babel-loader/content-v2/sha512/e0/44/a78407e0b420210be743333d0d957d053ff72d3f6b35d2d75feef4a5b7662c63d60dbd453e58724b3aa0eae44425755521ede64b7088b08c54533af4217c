file: null, type: null },\r\n    { file: null, type: null },\r\n    { file: null, type: null },\r\n    { file: null, type: null },\r\n    { file: null, type: null },\r\n  ]);\r\n  const router = useRouter();\r\n  const initialVis = [\r\n    { file: null, type: null },\r\n    { file: null, type: null },\r\n    { file: null, type: null },\r\n    { file: null, type: null },\r\n    { file: null, type: null },\r\n    { file: null, type: null },\r\n  ];\r\n  useEffect(() => {\r\n    const notEmpty = vis.filter((val) => {\r\n      return val.file !== null && val.type !== null;\r\n    });\r\n    // console.log(notEmpty);\r\n    if (notEmpty.length > 0) {\r\n      window.onbeforeunload = function () {\r\n        return \"Data will be lost if you leave the page, are you sure?\";\r\n      };\r\n    }\r\n  });\r\n  useEffect(() => {\r\n    if (state.visibilityReducer.visibility.length > 0) {\r\n      setVis([...state.visibilityReducer.visibility]);\r\n    }\r\n  }, [dispatch]);\r\n  useEffect(() => {\r\n    if (router.query.id) {\r\n      fetch(\r\n        `http://10.100.4.116:8230/api/ActivityVisitPlan/GetActivityVisitPlanById/${router.query.id}`\r\n      )\r\n        .then((response) => {\r\n          return response.json();\r\n        })\r\n        .then((data) => {\r\n          setPlan(data);\r\n          setLoading(false);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    }\r\n  }, [router.query.id]);\r\n\r\n  useEffect(() => {\r\n    if (router.query.id) {\r\n      fetch(\r\n        `http://10.100.4.116:8230/api/ActivityVisitPlanDPOSM/GetAllActivityVisitPlanDposm`\r\n      )\r\n        .then((response) => {\r\n          return response.json();\r\n        })\r\n        .then((data) => {\r\n          setPosm(data);\r\n          setLoading(false);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    }\r\n  }, [router.query.id]);\r\n\r\n  const renderInputUpload = () => {\r\n    var render = vis.map((val, index) => {\r\n      return (\r\n        <>\r\n          <div className={styles.visibility_grid}>\r\n            <div>POSM {index + 1}</div>\r\n            <div className={styles.visibility_dropdown}>\r\n              <Dropdown\r\n                options={posm}\r\n                onChange={(e) => {\r\n                  vis.splice(index, 1, {\r\n                    ...vis[index],\r\n                    type: e.target.value,\r\n                  });\r\n                  setDummy(dummy + 1);\r\n                }}\r\n                value={val.type != null ? val.type : \"\"}\r\n              />\r\n              <span\r\n                style={{\r\n                  fontSize: \"10px\",\r\n                  color: \"#41867A\",\r\n                  fontWeight: \"500\",\r\n                }}\r\n              >\r\n                {val.file != null ? val.file.name : \"\"}\r\n              </span>\r\n            </div>\r\n            <div>\r\n              <label className={styles.new_button} htmlFor={`upload${index}`}>\r\n                <img\r\n                  src={\"/camera.svg\"}\r\n                  style={{ width: \"18px\", verticalAlign: \"baseline\" }}\r\n                />\r\n                <input\r\n                  className={styles.input_file}\r\n                  onChange={(e) => {\r\n                    vis.splice(index, 1, {\r\n                      ...vis[index],\r\n                      file: e.target.files[0],\r\n                    });\r\n                    setDummy(dummy + 1);\r\n                  }}\r\n                  id={`upload${index}`}\r\n                  type=\"file\"\r\n                  accept=\"image/*\"\r\n                  capture=\"environment\"\r\n                />\r\n              </label>\r\n            </div>\r\n          </div>\r\n        </>\r\n      );\r\n    });\r\n    return render;\r\n  };\r\n  const onSave = () => {\r\n    // const doneForm = vis.filter((val) => {\r\n    //   return val.file !== null && val.type !== null;\r\n    // });\r\n    actions.setVisibility(vis);\r\n    Router.push(`/activity/visit/plan/${router.query.id}`);\r\n  };\r\n  const render = () => {\r\n    if (loading) {\r\n      return <Spinner />;\r\n    } else {\r\n      return (\r\n        <div className={styles.container}>\r\n          <div className={styles.wrapper}>\r\n            <Nav\r\n              title={\"Visibility\"}\r\n              // backHref={`/activity/visit/plan/${plan.id}`}\r\n              color={\"white\"}\r\n              action={\"Done\"}\r\n              backAction={() => {\r\n                if (state.visibilityReducer.visibility.length > 0) {\r\n                  setVis([...state.visibilityReducer.visibility]);\r\n                } else {\r\n                  setVis([...initialVis]);\r\n                }\r\n                window.onbeforeunload = function () {\r\n                  return \"Data will be lost if you leave the page, are you sure?\";\r\n                };\r\n                Router.push(`/activity/visit/plan/${router.query.id}`);\r\n              }}\r\n              onClick={() => {\r\n                onSave();\r\n              }}\r\n            />\r\n            <div className={styles.main}>{renderInputUpload()}</div>\r\n          </div>\r\n        </div>\r\n      );\r\n    }\r\n  };\r\n  return render();\r\n}\r\n"]},"metadata":{},"sourceType":"module"}