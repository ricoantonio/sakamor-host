ata)\r\n            .then((data) => {\r\n              setMasterPlanList(data);\r\n              console.log(data);\r\n              setLoading(false);\r\n            })\r\n            .catch((err) => {\r\n              console.log(err);\r\n            });\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    }\r\n  };\r\n\r\n  const renderMasterPlanList = () => {\r\n    return masterPlanList.map((val) => {\r\n      return (\r\n        <div\r\n          style={{\r\n            padding: \"10px 20px\",\r\n            fontSize: \"14px\",\r\n            display: \"grid\",\r\n            gridTemplateColumns: \"80% 20%\",\r\n          }}\r\n        >\r\n          <div>\r\n            <div style={{ fontWeight: \"500\" }}>{val.namaOutlet}</div>\r\n            <div style={{ fontWeight: \"400\", color: \"#B9B9C3\" }}>\r\n              {moment(val.tanggal).format(\"DD MMMM YYYY\")}\r\n            </div>\r\n          </div>\r\n          <div\r\n            style={{ textAlign: \"end\", margin: \"auto 0\" }}\r\n            onClick={() => {\r\n              console.log(val);\r\n              onDeleteVisit(val.id);\r\n            }}\r\n          >\r\n            <img src=\"/trash-2.svg\" />\r\n          </div>\r\n        </div>\r\n      );\r\n    });\r\n  };\r\n\r\n  const render = () => {\r\n    if (loading) {\r\n      return <Spinner />;\r\n    } else {\r\n      return (\r\n        <>\r\n          {renderModalAdd()}\r\n          <div className={styles.container}>\r\n            <div className={styles.wrapper}>\r\n              <Nav backHref=\"/\" />\r\n              <div className={styles.title_container}>\r\n                <div className={styles.title}>Master Plan Visit</div>\r\n              </div>\r\n              <div className={styles.main}>\r\n                <div className={styles.input_container}>\r\n                  <div style={{ marginRight: \"5px\" }}>\r\n                    <input\r\n                      className={styles.i