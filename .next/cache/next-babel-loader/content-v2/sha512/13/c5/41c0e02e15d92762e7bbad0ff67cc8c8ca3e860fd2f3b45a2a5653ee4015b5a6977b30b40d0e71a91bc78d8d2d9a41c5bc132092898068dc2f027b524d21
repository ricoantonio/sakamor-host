       : [];\n    const parentMenu = menuList.filter((val) => {\n      return val.isParentMenu === true;\n    });\n    const render = parentMenu.map((val, index) => {\n      return (\n        <Link href={\"/\" + val.moduleName.toLowerCase()} key={index}>\n          <a className={styles.menu_container}>\n            <img className={styles.menu_img} src={val.iconClass} />\n            <div className={styles.menu_text}>{val.moduleName}</div>\n          </a>\n        </Link>\n      );\n    });\n    return render;\n  };\n\n  const renderTarget = (type, now, max) => {\n    return (\n      <>\n        <div className={styles.target_box_grid}>\n          <img\n            className={styles.target_icon}\n            src={\n              type === \"Store Visibility\"\n                ? \"/search.svg\"\n                : type === \"Call\"\n                ? \"/phone.svg\"\n                : type === \"NOO\"\n                ? \"/protest.svg\"\n                : type === \"Sales Target\"\n                ? \"/target.svg\"\n                : null\n            }\n          />\n          <div style={{ marginLeft: \"10px\" }}>\n            <div className={styles.target_title_grid}>\n              <div className={styles.target_title}>{type}</div>\n              <div className={styles.target_details}>Details</div>\n            </div>\n            <div className={styles.target_now_text}>\n              {now.toLocaleString()}{\" \"}\n              <span className={styles.target_max_text}>\n                / {max.toLocaleString()}\n              </span>\n            </div>\n            <div>\n              <div className={styles.progress_bar}></div>\n              <div\n                className={styles.progress_bar_now}\n                style={{\n                  width: `${(now / max) * 100}%`,\n                }}\n              ></div>\n            </div>\n          </div>\n        </div>\n      </>\n    );\n  };\n\n  const renderPage = () => {\n    if (state.menuReducer.menu.length == 0) {\n      return <Spinner />;\n    } else\n      return (\n        <div>\n          <Head>\n            <title>Home</title>\n            <link rel=\"icon\" href=\"/favicon.ico\" />\n          </Head>\n          <div className={styles.wrapper}>\n            {/* nav */}\n            <div className={styles.bg_top}>\n              <div className={styles.grid_nav}>\n                <img className={styles.pp} src=\"/pp.png\" />\n                <img className={styles.nav_icon} src=\"/chat.svg\" />\n                <img className={styles.nav_icon} src=\"/more.svg\" />\n              </div>\n              <div className={styles.user_info}>\n                <div className={styles.user}>\n                  Welcome back, {state.userReducer.user.name}!\n                </div>\n                <div className={styles.user_info_grid}>\n                  <div className={styles.detail_grid}>\n                    <img className={styles.detail_img} src=\"/person.svg\" />\n                    <div className={styles.detail_txt}>Sales</div>\n                  </div>\n                  <div className={styles.detail_grid}>\n                    <img className={styles.detail_img} src=\"/apartment.svg\" />\n                    <div className={styles.detail_txt}>Kalimalang</div>\n                  </div>\n                  <div className={styles.detail_grid}>\n                    <img className={styles.detail_img} src=\"/place.svg\" />\n                    <div className={styles.detail_txt}>DKI Jakarta</div>\n                  </div>\n                </div>\n              </div>\n            </div>\n            {/* main menu */}\n            <div className={styles.main_menu_container}>\n              <div className={styles.top_menu_container}>\n                {renderMainMenu()}\n              </div>\n              {/* <div className={styles.date_range_container}>\n              <div className={styles.date_range_grid}>\n                <img className={styles.date_icon} src=\"/date_range.svg\" />\n                <div>12/01/2020 - 12/03/2020</div>\n              </div>\n            </div>\n            <div className={styles.subtitle}>Target</div>\n            {renderTarget(\"Call\", 3, 10)}\n            {renderTarget(\"Store Visibility\", 5, 8)}\n            {renderTarget(\"NOO\", 0, 8)}\n            {renderTarget(\"Sales Target\", 10, 12)}\n            <div className={styles.subtitle}>FBO3X</div> */}\n            </div>\n          </div>\n        </div>\n      );\n  };\n  return renderPage();\n}\n"]},"metadata":{},"sourceType":"module"}