 && loadingMenu) {\r\n      return <Spinner />;\r\n    } else {\r\n      return (\r\n        <div>\r\n          <Head>\r\n            <title>Home</title>\r\n            <link rel=\"icon\" href=\"/favicon.ico\" />\r\n          </Head>\r\n          <div className={styles.container}>\r\n            <div className={styles.wrapper}>\r\n              <div className={styles.bg_top} />\r\n              <div className={styles.user_info_grid}>\r\n                <img className={styles.pp} src=\"/pp.png\" />\r\n                <div style={{ marginLeft: \"15px\" }}>\r\n                  <div style={{ fontSize: \"14px\" }}>\r\n                    {state.userReducer.user.name}\r\n                  </div>\r\n                  <div style={{ fontSize: \"12px\" }}>{role}</div>\r\n                </div>\r\n                <div>\r\n                  <Link href={\"/announcement\"}>\r\n                    <a style={{ textDecoration: \"none\" }}>\r\n                      <img className={styles.notif} src=\"/notif.svg\" />\r\n                      {notif ? (\r\n                        <div\r\n                          style={{\r\n                            position: \" relative\",\r\n                            top: \"-30px\",\r\n                            right: \"-14px\",\r\n                          }}\r\n                        >\r\n                          <div\r\n                            style={{\r\n                              width: \"21px\",\r\n                              height: \"18px\",\r\n                              borderRadius: \"20px\",\r\n                              backgroundColor: \"#feb800\",\r\n                              position: \"relative\",\r\n                              color: \"white\",\r\n                              fontSize: \"12px\",\r\n                            }}\r\n                          >\r\n                            <div style={{ textAlign: \"center\" }}>\r\n                              {newAnnouncement.length < 9\r\n                                ? newAnnouncement.length\r\n                                : \"9+\"}\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                      ) : null}\r\n                    </a>\r\n                  </Link>\r\n                </div>\r\n                <div className={styles.main}>\r\n                  {renderTopMenu()}\r\n                  {focus === \"PLAN\"\r\n                    ? renderPlan()\r\n                    : focus === \"UNPLAN\"\r\n                    ? renderUnplan()\r\n                    : focus === \"SPREADING\"\r\n                    ? renderSpreading()\r\n                    : focus === \"WORK-VISIT\"\r\n                    ? renderWorkVisit()\r\n                    : \"\"}\r\n                  {renderWorkDay()}\r\n                  {renderProgress()}\r\n                  <div style={{ marginBottom: \"120px\" }} />\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <BotNav />\r\n        </div>\r\n      );\r\n    }\r\n  };\r\n  return renderPage();\r\n}\r\n"]},"metadata":{},"sourceType":"module"}