nst [programWeekList, setProgramWeekList] = useState([]);\r\n  const [now, setNow] = useState(new Date());\r\n\r\n  var dayName = moment(now).format(\"dddd\");\r\n  var date = moment(now).format(\"D\");\r\n  var monthName = moment(now).format(\"MMMM\");\r\n  var year = moment(now).format(\"YYYY\");\r\n\r\n  useEffect(() => {\r\n    var date = moment(now).format(\"YYYY-MM-DD\");\r\n    if (focus == \"Day\") {\r\n      getDayProgram(date)\r\n        .then((data) => {\r\n          setProgramList(data);\r\n          // console.log(\"program\", data);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n      getDayPromo(date)\r\n        .then((data) => {\r\n          setPromoList(data);\r\n          // console.log(\"promo\", data);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    } else if (focus === \"Week\") {\r\n    } else if (focus === \"Month\") {\r\n    }\r\n  }, [now, focus]);\r\n\r\n  const renderTopMenu = () => {\r\n    const menu = [\"Day\", \"Week\", \"Month\"];\r\n    return menu.map((val, index) => {\r\n      return (\r\n        <div\r\n          key={index}\r\n          onClick={() => {\r\n            setFocus(val);\r\n            setNow(new Date());\r\n          }}\r\n          className={\r\n            focus === val ? styles.menu_active : styles.menu_non_active\r\n          }\r\n        >\r\n          {val}\r\n        </div>\r\n      );\r\n    });\r\n  };\r\n\r\n  const renderDayList = (type, data) => {\r\n    return data.map((val) => {\r\n      return (\r\n        <div\r\n          style={{\r\n            fontSize: \"14px\",\r\n            fontWeight: \"500\",\r\n            color: \"#5E5873\",\r\n            textAlign: \"left\",\r\n            margin: \"10px 0\",\r\n          }}\r\n        >\r\n          <div\r\n            style={{\r\n              display: \"grid\",\r\n              gridTemplateColumns: \"12% 88%\",\r\n            }}\r\n          >\r\n            <div\r\n              style={{\r\n                width: \"20px\",\r\n                height: \"20px\",\r\n                backgroundColor: type === \"PROMO\" ? \"#d1e4e1\" : \"#FFF1CC\",\r\n                borderRadius: \"20px\",\r\n                padding: \"4px\",\r\n              }}\r\n            >\r\n              <div\r\n                style={{\r\n                  width: \"12px\",\r\n                  height: \"12px\",\r\n                  backgroundColor: type === \"PROMO\" ? \"#41867a\" : \"#feb800\",\r\n                  borderRadius: \"20px\",\r\n                }}\r\n              />\r\n            </div>\r\n            <div>\r\n              <div>{val.namaProduk}</div>\r\n              <div style={{ fontSize: \"12px\", fontWeight: \"300\" }}>\r\n                {val.deskripsi}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      );\r\n    });\r\n  };\r\n  const renderMenuDay = () => {\r\n    return (\r\n      <>\r\n        <Card style={{ borderRadius: \"6px\", marginTop: \"12px\" }}>\r\n          <div style={{ padding: \"22px 0 0 22px\" }}>\r\n            <span className={styles.day_promo_title}>Promo</span>\r\n          </div>\r\n          <div style={{ padding: \"6px 20px\" }}>\r\n            {renderDayList(\"PROMO\", promoList)}\r\n          </div>\r\n        </Card>\r\n        <Card style={{ borderRadius: \"6px\", marginTop: \"12px\" }}>\r\n          <div style={{ padding: \"22px 0 0 22px\" }}>\r\n            <span className={styles.day_program_title}>Program</span>\r\n          </div>\r\n          <div style={{ padding: \"6px 20px\" }}>\r\n            {renderDayList(\"PROGRAM\", programList)}\r\n          </div>\r\n        </Card>\r\n      </>\r\n    );\r\n  };\r\n  const renderWeekList = (type, data) => {\r\n    return data.map((val) => {\r\n      return (\r\n        <div className={styles.week_main_container}>\r\n          No promo or program today\r\n          <div\r\n            style={{\r\n              fontSize: \"14px\",\r\n              fontWeight: \"500\",\r\n              color: \"#5E5873\",\r\n              textAlign: \"left\",\r\n              margin: \"10px 0\",\r\n            }}\r\n          >\r\n            <div\r\n              style={{\r\n                display: \"grid\",\r\n                gridTemplateColumns: \"12% 88%\",\r\n              }}\r\n            >\r\n              <div\r\n                style={{\r\n                  width: \"20px\",\r\n                  height: \"20px\",\r\n                  backgroundColor: type === \"PROMO\" ? \"#d1e4e1\" : \"#FFF1CC\",\r\n                  borderRadius: \"20px\",\r\n                  padding: \"4px\",\r\n                }}\r\n              >\r\n                <div\r\n                  style={{\r\n                    width: \"12px\",\r\n                    height: \"12px\",\r\n                    backgroundColor: type === \"PROMO\" ? \"#41867a\" : \"#feb800\",\r\n                    borderRadius: \"20px\",\r\n                  }}\r\n                />\r\n              </div>\r\n              <div>\r\n                <div>{val.namaProduk}</div>\r\n                <div style={{ fontSize: \"12px\", fontWeight: \"300\" }}>\r\n                  {val.deskripsi}\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      );\r\n    });\r\n  };\r\n  const renderMenuWeek = () => {\r\n    const arrWeek = [];\r\n    const promoWeekList = [];\r\n    const programWeekList = [];\r\n    const dateStart = moment().startOf(\"isoweek\");\r\n    for (let i = 0; i < 5; i++) {\r\n      const date = moment(dateStart).add(\"days\", i);\r\n      var dateParameter = moment(date).format(\"YYYY-MM-DD\");\r\n\r\n      getDayProgram(dateParameter)\r\n        .then((data) => {\r\n          programWeekList.push(data);\r\n          getDayPromo(dateParameter)\r\n            .then((data) => {\r\n              promoWeekList.push(data);\r\n              arrWeek.push(date);\r\n              if (promoWeekList.length === 5 && programWeekList.length === 5) {\r\n                setLoadingWeek(false);\r\n              }\r\n            })\r\n            .catch((err) => {\r\n              console.log(err);\r\n            });\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    }\r\n    return arrWeek.map((val, index) => {\r\n      if (loadingWeek) {\r\n        return <Spinner />;\r\n      } else {\r\n        return (\r\n          <div key={index}>\r\n            <div className={styles.week_header_container}>\r\n              <div>{moment(val).format(\"MMMM D, YYYY\")}</div>\r\n              <div>{moment(val).format(\"dddd\")}</div>\r\n            </div>\r\n            <div>\r\n              {renderWeekList(\"PROMO\", promoWeekList[index])}\r\n              {renderWeekList(\"PROGRAM\", programWeekList[index])}\r\n            </div>\r\n          </div>\r\n        );\r\n      }\r\n    });\r\n  };\r\n  const renderMenuMonth = () => {\r\n    return (\r\n      <div>\r\n        <div className={styles.month_header_container}>\r\n          <div\r\n            onClick={() => {\r\n              setNow(moment(now).subtract(1, \"months\"));\r\n            }}\r\n          >\r\n            <img src={\"/prev_white.svg\"} />\r\n          </div>\r\n          <div>\r\n            {moment(now).format(\"MMMM\")}\r\n            <span style={{ fontWeight: \"300\" }}>\r\n              {\" \"}\r\n              {moment(now).format(\"YYYY\")}\r\n            </span>\r\n          </div>\r\n          <div\r\n            onClick={() => {\r\n              setNow(moment(now).add(1, \"months\"));\r\n            }}\r\n          >\r\n            <img src={\"/next_white.svg\"} />\r\n          </div>\r\n        </div>\r\n        <div className={styles.week_main_container}>\r\n          No promo or program this month\r\n        </div>\r\n      </div>\r\n    );\r\n  };\r\n\r\n  const togglePromo = () => {\r\n    setPromo(!promo);\r\n  };\r\n  const toggleProgram = () => {\r\n    setProgram(!program);\r\n  };\r\n\r\n  const renderPage = () => {\r\n    if (loading) {\r\n      return <Spinner />;\r\n    } else\r\n      return (\r\n        <div>\r\n          <Head>\r\n            <title>Home</title>\r\n            <link rel=\"icon\" href=\"/favicon.ico\" />\r\n          </Head>\r\n          <div className={styles.container}>\r\n            <div className={styles.wrapper}>\r\n              <Nav title={\"Calendar\"} color={\"white\"} noBack />\r\n              <div className={styles.main}>\r\n                <Card style={{ borderRadius: \"6px\" }}>\r\n                  <div className={styles.menu_grid}>{renderTopMenu()}</div>\r\n                  {focus === \"Day\" ? (\r\n                    <>\r\n                      <div className={styles.day_title}>{dayName}</div>\r\n                      <div className={styles.day_grid}>\r\n                        <div\r\n                          onClick={() => {\r\n                            setNow(moment(now).subtract(1, \"days\"));\r\n                          }}\r\n                        >\r\n                          <img src={\"/cev_left.svg\"} />\r\n                        </div>\r\n                        <div>\r\n                          <div className={styles.day_date_display}>{date}</div>\r\n                        </div>\r\n                        <div\r\n                          onClick={() => {\r\n                            setNow(moment(now).add(1, \"days\"));\r\n                          }}\r\n                        >\r\n                          <img src={\"/cev_right.svg\"} />\r\n                        </div>\r\n                      </div>\r\n                      <div className={styles.day_bot_display}>\r\n                        <div>{monthName}</div>\r\n                        <div>{year}</div>\r\n                      </div>\r\n                    </>\r\n                  ) : (\r\n                    <div className={styles.filter_container}>\r\n                      <div className={styles.filter_text}>FILTER</div>\r\n                      <Checkbox\r\n                        text={\"Promo\"}\r\n                        checked={promo}\r\n                        color={\"green\"}\r\n                        onClick={() => togglePromo()}\r\n                      />\r\n                      <Checkbox\r\n                        text={\"Program\"}\r\n                        checked={program}\r\n                        color={\"yellow\"}\r\n                        onClick={() => toggleProgram()}\r\n                      />\r\n                    </div>\r\n                  )}\r\n                </Card>\r\n                {focus === \"Day\"\r\n                  ? renderMenuDay()\r\n                  : focus === \"Week\"\r\n                  ? renderMenuWeek()\r\n                  : focus === \"Month\"\r\n                  ? renderMenuMonth()\r\n                  : \"\"}\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <BotNav />\r\n        </div>\r\n      );\r\n  };\r\n  return renderPage();\r\n}\r\n"]},"metadata":{},"sourceType":"module"}