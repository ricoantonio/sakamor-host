pan>\n                <div style={{ color: \"#5E5873\", marginTop: \"7px\" }}>\n                  <span style={{ fontSize: \"36px\", fontWeight: \"600\" }}>0</span>\n                  <span style={{ fontSize: \"18px\" }}>/{plan.length}</span>\n                </div>\n                <button\n                  style={{\n                    marginTop: \"7px\",\n                    backgroundColor: \"#FEB800\",\n                    padding: \"10px 22px\",\n                    fontSize: \"14px\",\n                    fontWeight: \"400\",\n                    color: \"#fff\",\n                    border: \"none\",\n                    borderRadius: \"5px\",\n                  }}\n                >\n                  Add New Plan +\n                </button>\n              </div>\n              <div></div>\n            </div>\n          </Card>\n          <Card style={{ marginTop: \"22px\", borderRadius: \"30px\" }}>\n            <div className={styles.plan_container}>\n              <div\n                style={{\n                  fontSize: \"18px\",\n                  fontWeight: \"500\",\n                  color: \"#5E5873\",\n                  textAlign: \"left\",\n                }}\n              >\n                Your Plan Today\n              </div>\n              <div style={{ margin: \"22px 0\" }}>{planMap}</div>\n              <Link href=\"/visit/plan\">\n                <a>\n                  <Button text={\"See Details\"} />\n                </a>\n              </Link>\n            </div>\n          </Card>\n        </>\n      );\n    }\n  };\n\n  const renderSpreading = () => {\n    const planMap = plan.map((val, index) => {\n      return (\n        <div\n          style={{\n            fontSize: \"15px\",\n            fontWeight: \"700\",\n            color: \"#5E5873\",\n            textAlign: \"left\",\n            margin: \"10px 0\",\n          }}\n          key={index}\n        >\n          <div>{val.namaOutlet}</div>\n          <div style={{ fontSize