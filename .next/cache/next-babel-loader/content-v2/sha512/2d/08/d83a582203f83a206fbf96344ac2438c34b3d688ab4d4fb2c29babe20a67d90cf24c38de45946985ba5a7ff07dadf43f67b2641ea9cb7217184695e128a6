List, setPromoWeekList] = useState([]);\r\n  const [programWeekList, setProgramWeekList] = useState([]);\r\n  const [now, setNow] = useState(new Date());\r\n\r\n  var dayName = moment(now).format(\"dddd\");\r\n  var date = moment(now).format(\"D\");\r\n  var monthName = moment(now).format(\"MMMM\");\r\n  var year = moment(now).format(\"YYYY\");\r\n\r\n  useEffect(() => {\r\n    var date = moment(now).format(\"YYYY-MM-DD\");\r\n    if (focus == \"Day\") {\r\n      getDayProgram(date)\r\n        .then((data) => {\r\n          setProgramList(data);\r\n          // console.log(\"program\", data);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n      getDayPromo(date)\r\n        .then((data) => {\r\n          setPromoList(data);\r\n          // console.log(\"promo\", data);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    } else if (focus === \"Week\") {\r\n    } else if (focus === \"Month\") {\r\n    }\r\n  }, [now, focus]);\r\n\r\n  const renderTopMenu = () => {\r\n    const menu = [\"Day\", \"Week\", \"Month\"];\r\n    return menu.map((val, index) => {\r\n      return (\r\n        <div\r\n          key={index}\r\n          onClick={() => {\r\n            setFocus(val);\r\n            setNow(new Date());\r\n          }}\r\n          className={\r\n            focus === val ? styles.menu_active : styles.menu_non_active\r\n          }\r\n        >\r\n          {val}\r\n        </div>\r\n      );\r\n    });\r\n  };\r\n\r\n  const renderDayList = (type, data) => {\r\n    return data.map((val) => {\r\n      return (\r\n        <div\r\n          style={{\r\n            fontSize: \"14px\",\r\n            fontWeight: \"500\",\r\n            color: \"#5E5873\",\r\n            textAlign: \"left\",\r\n            margin: \"10px 0\",\r\n          }}\r\n        >\r\n          <div\r\n            style={{\r\n              display: \"grid\",\r\n              gridTemplateColumns: \"12% 88%\",\r\